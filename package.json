    "email-validator": "2.0.4",
    "express": "^4.21.2",
    "fluent-ffmpeg": "2.1.2",
    "google-auth-library": "^9.15.0",
    "googleapis": "144.0.0",
    "luxon": "3.5.0",
    "mime-types": "2.1.35",
    "node-cron": "^3.0.3",
    "node-fetch": "3.3.2",
